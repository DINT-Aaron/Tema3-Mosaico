<Window x:Class="Tema3_Mosaico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tema3_Mosaico"
        mc:Ignorable="d"
        Title="Mosaico" Icon="assets/32.ico" MinWidth="450" MinHeight="470" Height="450" Width="800">
    <DockPanel>
        <TextBlock DockPanel.Dock="Left" Width="100" Background="Purple"></TextBlock>
        <TextBlock DockPanel.Dock="Top" Height="100" Background="Purple"></TextBlock>
        <TextBlock DockPanel.Dock="Right" Width="100" Background="Green"></TextBlock>
        <TextBlock DockPanel.Dock="Bottom" Height="100" Background="Green"></TextBlock>
        <TextBlock DockPanel.Dock="Left" Width="100" Background="Orange"></TextBlock>
        <TextBlock DockPanel.Dock="Bottom" Height="100" Background="Orange"></TextBlock>
        <TextBlock DockPanel.Dock="Top" Height="100" Background="Pink"></TextBlock>
        <TextBlock DockPanel.Dock="Right" Width="100" Background="Pink"></TextBlock>
        <TextBlock Background="Black"></TextBlock>
    </DockPanel>
</Window>
